<header>
  <h1>Senaste pressreleaser</h1>
</header>

<nav id="pressreleases">
  <ul>
  <% for pressrelease in @pressreleases %>
      <li><%= link_to pressrelease.at_css("header").text, news_path(pressrelease.at_css("id").text) %></li>
  <% end %>
  </ul>
</nav>
<nav id="pagination">
  <%= link_to "next", news_index_path(:page => "2"), :id => "next" %>
</nav>
